<!doctype html>
<html lang="en">
<head>

    <meta charset="utf-8" />
    <meta name="viewport" content="width=device-width, initial-scale=1, shrink-to-fit=no">

    <title>@ViewData["Title"] - SamProject</title>

    <environment include="Development">
        <link rel="stylesheet"
              href="~/vendor/bootstrap/dist/css/bootstrap.min.css" />
        <link rel="stylesheet"
              href="~/vendor/syncfusion-javascript/Content/ej/web/bootstrap-theme/ej.web.all.min.css" />
        <link rel="stylesheet"
              href="~/vendor/syncfusion-javascript/Content/ej/web/responsive-css/ej.responsive.css" />
    </environment>

    <environment exclude="Development">
        <link rel="stylesheet"
              href="~/lib/bootstrap/dist/css/bootstrap.min.css" />
        <link rel="stylesheet"
              href="~/lib/syncfusion-javascript/Content/ej/web/bootstrap-theme/ej.web.all.min.css" />
        <link rel="stylesheet"
              href="~/lib/syncfusion-javascript/Content/ej/web/responsive-css/ej.responsive.css" />
    </environment>

    <link rel="stylesheet"
          href="~/css/site.min.css"
          asp-append-version="true" />
    


</head>
<body style="padding-top:0;">

    @*<partial name="_CookieConsentPartial" />*@

    <div style="margin: 10px 5%; height: 100%; ">
        @RenderBody()
    </div>

    <environment include="Development">
        <script src="~/vendor/jquery/dist/jquery.min.js"></script>
        <script src="~/vendor/popper.js/dist/umd/popper.min.js"></script>
        <script src="~/vendor/bootstrap/dist/js/bootstrap.min.js"></script>
        <script src="~/vendor/jsrender/jsrender.min.js"></script>
        <script src="~/vendor/syncfusion-javascript/Scripts/ej/web/ej.web.all.min.js"></script>
    </environment>

    <environment exclude="Development">
        <script src="~/lib/jquery/dist/jquery.min.js"></script>
        <script src="~/lib/popper.js/dist/umd/popper.min.js"></script>
        <script src="~/lib/bootstrap/dist/css/bootstrap.min.js"></script>
        <script src="~/lib/jsrender/jsrender.min.js"></script>
        <script src="~/lib/syncfusion-javascript/Scripts/ej/web/ej.web.all.min.js"></script>
    </environment>

    <script src="~/js/site.js" asp-append-version="true"></script>

    <ej-script-manager></ej-script-manager>

    @RenderSection("Scripts", required: false)
</body>
</html>
